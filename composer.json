{
  "name": "triquang/laravel-model-doc",
  "description": "Generates PHPDoc annotations (@property, etc.) for Eloquent models, supporting both standard and modular Laravel applications.",
  "version": "1.0.1",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nguyễn Trí Quang",
      "email": "ntquangkk@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "TriQuang\\LaravelModelDoc\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TriQuang\\LaravelModelDoc\\LaravelModelDocServiceProvider"
      ]
    }
  },
  "keywords": [
    "laravel",
    "module",
    "modular",
    "model",
    "property",
    "docblock",
    "phpdoc",
    "PHPdoc",
    "annotation",
    "generator"
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}
